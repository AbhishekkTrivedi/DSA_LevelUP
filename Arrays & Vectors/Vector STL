#include <iostream>
#include <vector>
using namespace std;

int main(){

    vector<int> arr = {16,20,34,41,57,68};
    //Push_Back
    arr.push_back(14);
    //Print all the elements
    for (int i=0; i <arr.size();i++){
        cout<< arr[i] << endl;
    }
--------------------------------------------------------------------------------------------------------------------------------
16
20
34
41
57
68
14
---------------------------------------------------------------------------------------------------------------------------------

    //Pop_back -> Remove last element 
    arr.pop_back();

    for (int i=0; i <arr.size();i++){
        cout<< arr[i] << endl;
    }
------------------------------------------------------------------------------------------------------------------------------------
16
20
34
41
57
68
-----------------------------------------------------------------------------------------------------------------------------------------

    //Size of vector
    cout<< arr.size() <<endl;
-----------------------------------------------------------------------------------------------------------------------------------------
6
----------------------------------------------------------------------------------------------------------------------------------------

    //Capacity tells us size of assingned memory
    cout<< arr.capacity() << endl;
---------------------------------------------------------------------------------------------------------------------------------------
12
----------------------------------------------------------------------------------------------------------------------------------------
    
    //Fill Constructor
    vector<int>Abhi(10,3);
    //
    for (int j=0; j <Abhi.size();j++){
        cout<< Abhi[j] << endl;
    }
--------------------------------------------------------------------------------------------------------------------------------------------
3
3
3
3
3
3
3
3
3
3
-----------------------------------------------------------------------------------------------------------------------------------------------

    return 0;
}
